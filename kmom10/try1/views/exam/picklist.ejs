<%- include("../partials/header", {title: title, path: path}); %>

<h1>Plocklista</h1>
<p>
    Order id: <%= order_id %>, Kund id: <%= kund_id %> 
</p>
<section>
    <table>
        <thead>
            <tr>
                <th>Produkt id</th>
                <th>Produktnamn</th>
                <th>Antal beställda</th>
                <th>Antal på hyllan</th>
                <th># att plocka</th>
                <th>Från hylla</th>
            </tr>
        </thead>
        <tbody>
        <% for (const row of res) { %>
            <tr>
                <td class="right"><%= row["ProductID"] %></td>
                <td class="right"><%= row["Product name"] %></td>
                <td class="right"><%= row["Ordered"] %></td>
                <td class="right"><%= row["Nr on shelf"] %></td>
                <td class="right"><%= row["Number to pick"] %></td>
                <td class="right"><%= row["From shelf"] %></td>
                </td>
            </tr>
        <% } %>
        </tbody>
    </table>
</section>

<%- include("../partials/footer"); %>
