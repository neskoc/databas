<%- include("header", {title: title, path: path}); %>

<h1>Account balance</h1>

<p>Here is the balance for the accounts.</p>

<table>
    <tr>
        <th>Id</th>
        <th>Name</th>
        <th>Balance</th>
    </tr>
<% for (const row of res) { %>
    <tr>
        <td><%= row.id %></td>
        <td><%= row.name %></td>
        <td class="right"><%= row.balance %></td>
    </tr>
<% }; %>
</table>

<%- include("footer"); %>
